{
  "manifest_version": 2,

  "name": "Wordpress Information",
  "description": "Get worpress class and page/post id and link to edit page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Wordpress Information"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}