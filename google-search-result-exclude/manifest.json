/* https://developer.chrome.com/extensions/getstarted */

{
  "manifest_version": 2,

  "name": "Google Search Result Excluder",
  "description": "This extension allows the user to exclude domains from search results.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Google Search Result Excluder"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}