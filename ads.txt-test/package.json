{
  "name": "ads.txt-watch-tool",
  "version": "1.3.10",
  "devDependencies": {
    "eslint-plugin-json": "^1.2.1",
    "grunt": "^1.0.3",
    "grunt-cli": "^1.3.1",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^21.0.0",
    "grunt-newer": "^1.3.0",
    "grunt-sass": "^3.0.1",
    "grunt-version": "^1.3.0",
    "jquery": "^3.2.1",
    "load-grunt-tasks": "^4.0.0",
    "node-sass": "^4.9.4"
  },
  "scripts": {
    "clearlock": "clear-git-lock.cmd",
    "verpat": "grunt version:pat && grunt build",
    "vermin": "grunt version:min && grunt build",
    "vermaj": "grunt version:maj && grunt build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "plugins": [
      "eslint-plugin-json"
    ],
    "rules": {
      "no-console": 0,
      "no-const-assign": "warn",
      "no-this-before-super": "warn",
      "no-undef": "warn",
      "no-unreachable": "warn",
      "no-unused-vars": "warn",
      "constructor-super": "warn",
      "valid-typeof": "warn"
    },
    "globals": {
      "chrome": false,
      "sites": true
    }
  },
  "dependencies": {}
}
